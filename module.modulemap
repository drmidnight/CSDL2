module CSDL2 {
        header "/usr/include/SDL2/SDL.h"
        link "SDL2"
    export *

    module image {
    	header "/usr/include/SDL2/SDL_image.h"
    	link "SDL2_image"
    	export *
    }
    
    module ttf {
        header "/usr/include/SDL2/SDL_ttf.h"
        link "SDL2_ttf"
        export *
    }
}
